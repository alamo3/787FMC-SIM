public void setText(String x, String y, String z){
 label1.setText("Waypoints for SID: "+y+" Runway: "+z+" At ICAO: "+x);   
}

public void setList(List<String> waypoints){
    for(int i=0;i<waypoints.size();i++){
     list1.add(waypoints.get(i));   
    }
